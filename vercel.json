{
    "headers": [
      {
        "source": "/(.*)",
        "headers" : [
          {
            "key" : "Origin Control",
            "value" : "Off"
          },
          {
            "key" : "X-Frame-Options",
            "value" : "DENY"
          },
          {
            "key" : "X-XSS-Protection",
            "value" : "1; mode=block"
          },
          {
            "key" : "strict-transport-security",
            "value" : "max-age=0"
          }
        ]
      }
    ]
   "rewrites":  [
    {"source": "/(.*)", "destination": "/"}
  ]
  }
